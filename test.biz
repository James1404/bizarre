const x = i32;

fn Vec2(T: type) type {
   return struct {
          x: T;
          y: T;
   };
}

fn factorial(n: i32) i32 {
      return if n < 2 {
             1
      } else {
             n * factorial(n - 1)
      };
}

fn fac12() i32 {
   return factorial(12);
}

fn double(x: i32) i32 {
   return x * 2;
}

