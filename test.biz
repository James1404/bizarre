const println = import("io").println;

const fac1 = fn(n) -> if n <= 1 then 1 else n * fac1(n - 1);

const double = fn (x) -> x * 2;

const factorial = fn(n) -> match {
      0 -> 1,
      1 -> 1,
      n -> n * factorial(n - 1),
};

const double = fn(x) -> x * 2;

const main = fn() -> {
      var n = 25;
      println(if n == 25 then "equal 25" else "wrong!");
      println(factorial(13));
      println(23.4); 
};
